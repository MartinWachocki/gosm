{
    "verbose": true,
    "check_interval": 30,
    "pending_offline_check_interval": 5,
    "max_concurrent_checks": 5,
    "icmp_timeout": 1500,
    "successful_http_status_codes": [200],
    "ignore_http_cert_errors": false,
    "failed_check_threshold": 5,
    "send_email": true,
    "email_recipients": ["johndoe@example.com"],
    "send_sms": true,
    "sms_recipients": ["+18001234567"],
    "twilio_account_sid": "YOUR_TWILIO_ACCOUNT_SID",
    "twilio_auth_token": "YOUR_TWILIO_AUTH_TOKEN",
    "twilio_phone_number": "YOUR_TWILIO_PHONE_NUMBER",
    "services": [
        {
            "name": "Example.com website",
            "protocol": "http",
            "host": "http://example.com/",
            "port": 80
        }
    ]
}